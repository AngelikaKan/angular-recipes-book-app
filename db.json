    {
      "id": 8,
      "title": "Carrot Cake",
      "cookingTimeInMinutes": 80,
      "imageUrl": "/assets/images/carrot_cake.jpg",
      "ingredients": [
        { "name": "flour", "quantity": 300, "measuringUnit": "g" },
        { "name": "salt", "quantity": 1, "measuringUnit": "pinch" },
        { "name": "baking powder", "quantity": 1, "measuringUnit": "tsp" },
        { "name": "grated carrots", "quantity": 240, "measuringUnit": "g" },
        { "name": "eggs", "quantity": 3, "measuringUnit": "pcs" },
        { "name": "sunflower oil", "quantity": 200, "measuringUnit": "ml" },
        { "name": "granulated sugar", "quantity": 250, "measuringUnit": "g" },
        { "name": "cinnamon powder", "quantity": 2, "measuringUnit": "tsp" },
        { "name": "chopped walnuts", "quantity": 50, "measuringUnit": "g" },
        { "name": "cream cheese", "quantity": 300, "measuringUnit": "g" },
        { "name": "powdered sugar", "quantity": 150, "measuringUnit": "g" },
        { "name": "vanilla extract", "quantity": 1, "measuringUnit": "tsp" }
      ],
      "methodSteps": [
        {
          "stepNumber": 1,
          "description": "Combine dry ingredients (except for sugar) together in a large bowl."
        },
        {
          "stepNumber": 2,
          "description": "Beat the egg, oil and granulated sugar in a large bowl."
        },
        {
          "stepNumber": 3,
          "description": "Add the dry ingredients and mix until just incorporated."
        },
        {
          "stepNumber": 4,
          "description": "Add the grated carrot and chopped walnuts until just combined."
        },
        {
          "stepNumber": 5,
          "description": "Pour the dough in two prepared tin and bake for 25 minutes."
        },
        {
          "stepNumber": 6,
          "description": "When ready, let them cool down for 5 minutes and put on wire rack to cool down completely."
        },
        {
          "stepNumber": 7,
          "description": "Beat the cream cheese, powdered sugar and vanilla in a large bowl until the mixture is smooth and soft."
        },
        {
          "stepNumber": 8,
          "description": "Decorate the cake with the frosting and serve."
        }
      ]
    },
