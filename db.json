    {
      "id": 6,
      "title": "Chicken Pie",
      "cookingTimeInMinutes": 75,
      "imageUrl": "/assets/images/chicken_pie.jpg",
      "ingredients": [
        { "name": "chicken thighs", "quantity": 2, "measuringUnit": "pcs" },
        { "name": "salt", "quantity": 10, "measuringUnit": "g" },
        { "name": "fresh thyme", "quantity": 3, "measuringUnit": "Tbsp" },
        { "name": "cream", "quantity": 30, "measuringUnit": "ml" },
        { "name": "cheddar", "quantity": 200, "measuringUnit": "g" },
        { "name": "butter", "quantity": 125, "measuringUnit": "g" },
        { "name": "flour", "quantity": 350, "measuringUnit": "g" },
        { "name": "egg", "quantity": 1, "measuringUnit": "pcs" },
        { "name": "cold water", "quantity": 30, "measuringUnit": "ml" }
      ],
      "methodSteps": [
        {
          "stepNumber": 1,
          "description": "Roast the chicken for 20 minutes in preheated oven at 190 degrees C."
        },
        {
          "stepNumber": 2,
          "description": "When ready, let it cool down and shred it to bitesize pieces in a large bowl."
        },
        {
          "stepNumber": 3,
          "description": "Add 5 g of salt (or to taste) and combine with cream, thyme and cheese."
        },
        {
          "stepNumber": 4,
          "description": "In a food processor, add the flour and butter and process until the butter looks like fine crumbs."
        },
        {
          "stepNumber": 5,
          "description": "Put the mixture in a bowl and add the water and egg."
        },
        {
          "stepNumber": 6,
          "description": "Knead into a smooth dough and roll it into two equal sheets."
        },
        {
          "stepNumber": 7,
          "description": "Place one of the sheets on the bottom of a pie dish and cover with the second sheet."
        },
        {
          "stepNumber": 8,
          "description": "Bake in a prepared oven at 200 degrees C for 40 minutes and serve hot."
        }
      ]
    },
    {
      "id": 7,
