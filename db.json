        { "name": "butter", "quantity": 125, "measuringUnit": "g" }
      ],
      "methodSteps": [
        {
          "stepNumber": 1,
          "description": "Roll the puff pastry more thinly and cut in two equal pieces."
        },
        {
          "stepNumber": 2,
          "description": "Put one of the pieces in a large dish, spread the tomato sauce, top with ham and finish with the grated cheddar."
        },
        {
          "stepNumber": 3,
          "description": "Place the remaining puff pastry on top and close around the edges to seal."
        },
        {
          "stepNumber": 4,
          "description": "Separate the egg yolk, beat it lightly and brush the surface of the pie."
        },
        {
          "stepNumber": 5,
          "description": "Bake in preheated oven at 180 degrees C for 20 minutes."
        }
